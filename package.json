{
    "name": "disflow",
    "packageManager": "yarn@4.12.0",
    "author": {
        "name": "DisFlow Team"
    },
    "scripts": {
        "build": "turbo build",
        "test": "turbo test",
        "lint": "turbo run lint",
        "dev": "turbo build --filter=!web-application && yarn workspace web-application dev",
        "prettier": "turbo run prettier",
        "prettier:check": "turbo run prettier:check",
        "build:packages": "turbo build --filter=!web-application"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "eslint": "^9.39.2",
        "prettier": "^3.7.4",
        "tsup": "^8.5.1",
        "turbo": "^2.6.3",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.50.0"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "type": "module",
    "dependencies": {
        "@types/node": "^25.0.3"
    }
}
